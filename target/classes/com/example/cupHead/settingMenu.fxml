<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0"
      prefWidth="1280.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.cupHead.fxController.SettingControllerFX">
    <MenuBar VBox.vgrow="NEVER">
        <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" text="New"/>
            <MenuItem mnemonicParsing="false" text="Open…"/>
            <Menu mnemonicParsing="false" text="Open Recent"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Close"/>
            <MenuItem mnemonicParsing="false" text="Save"/>
            <MenuItem mnemonicParsing="false" text="Save As…"/>
            <MenuItem mnemonicParsing="false" text="Revert"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Preferences…"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Quit"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem mnemonicParsing="false" text="Undo"/>
            <MenuItem mnemonicParsing="false" text="Redo"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Cut"/>
            <MenuItem mnemonicParsing="false" text="Copy"/>
            <MenuItem mnemonicParsing="false" text="Paste"/>
            <MenuItem mnemonicParsing="false" text="Delete"/>
            <SeparatorMenuItem mnemonicParsing="false"/>
            <MenuItem mnemonicParsing="false" text="Select All"/>
            <MenuItem mnemonicParsing="false" text="Unselect All"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" text="About MyHelloApp"/>
        </Menu>
    </MenuBar>
    <AnchorPane VBox.vgrow="ALWAYS">
        <Label alignment="CENTER" layoutX="155.0" layoutY="177.0" style="&#10;"
               text="Drag components from Library here…" textAlignment="CENTER" textFill="#9f9f9f">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <ImageView fitHeight="775.0" fitWidth="1380.0" layoutX="-50.0">
            <Image url="@../assets/backgrounds/settingBackground.jpg"/>
        </ImageView>
        <Button fx:id="hardButton" layoutX="244.0"
                layoutY="322.0" mnemonicParsing="false" text="no-Head"
                onAction="#noHeadButton"/>
        <Text fx:id="currentDifficultyText" fill="WHITE"
              layoutX="39.0" layoutY="91.0"
              text="Current Difficulty: " wrappingWidth="896.13671875">
            <font>
                <Font size="57.0"/>
            </font>
        </Text>
        <Button fx:id="easyButton" onAction="#cupHeadButton"
                layoutX="39.0" layoutY="322.0" mnemonicParsing="false"
                text="cup-Head"/>
        <Button fx:id="normalButton" onAction="#plasticCupHeadButton"
                layoutX="126.0" layoutY="322.0"
                mnemonicParsing="false"
                text="plasticCup-Head"/>
        <Text fx:id="primitiveCupHeadHealthText" fill="WHITE"
              layoutX="39.0" layoutY="151.0"
              text="PrimitiveCupHeadHealth:" wrappingWidth="896.13671875">
            <font>
                <Font size="34.0"/>
            </font>
        </Text>
        <Text fx:id="gettingDamagedCoefficientText" fill="WHITE"
              layoutX="39.0" layoutY="216.0"
              text="GettingDamagedCoefficient:" wrappingWidth="896.13671875">
            <font>
                <Font size="34.0"/>
            </font>
        </Text>
        <Text fx:id="makingDamageCoefficientText" fill="WHITE"
              layoutX="39.0" layoutY="280.0"
              text="MakingDamageCoefficient: " wrappingWidth="896.13671875">
            <font>
                <Font size="34.0"/>
            </font>
        </Text>
        <Text fx:id="devilModeText" fill="WHITE" layoutX="98.0"
              layoutY="550.0"
              text="Devil Mode:" wrappingWidth="896.13671875">
            <font>
                <Font size="57.0"/>
            </font>
        </Text>
        <Button layoutX="23.0" layoutY="499.0" mnemonicParsing="false"
                prefHeight="63.0" prefWidth="69.0"
                onAction="#devilButton"/>
        <Text fx:id="soundText" fill="WHITE" layoutX="98.0"
              layoutY="675.0"
              text="Sound:" wrappingWidth="896.13671875">
            <font>
                <Font size="57.0"/>
            </font>
        </Text>
        <Button layoutX="23.0" layoutY="624.0" mnemonicParsing="false"
                prefHeight="63.0" prefWidth="69.0"
                onAction="#soundButton"/>
        <Button layoutX="1183.0" layoutY="15.0" mnemonicParsing="false"
                onAction="#mainMenu" text="main menu"/>
    </AnchorPane>
</VBox>
