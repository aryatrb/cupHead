<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0"
      prefWidth="1280.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.cuphead.fxcontroller.GameEndControllerFX">
    <AnchorPane VBox.vgrow="ALWAYS">
        <ImageView fitHeight="800.0" fitWidth="1280.0" layoutY="-14.0">
            <Image url="@../assets/backgrounds/gameEndBackground.png"/>
        </ImageView>
        <Button onMouseClicked="#backToMenu" layoutX="581.0"
                layoutY="563.0" mnemonicParsing="false" text="Back to Main Menu"/>
        <Button onMouseClicked="#replay" layoutX="598.0" layoutY="603.0"
                mnemonicParsing="false" text="Replay Game"/>
        <Text fx:id="time" layoutX="602.0" layoutY="452.0"
              strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
            <font>
                <Font size="41.0"/>
            </font>
        </Text>
        <Text fx:id="score" layoutX="602.0" layoutY="523.0"
              strokeType="OUTSIDE" strokeWidth="0.0" text="Text">
            <font>
                <Font size="41.0"/>
            </font>
        </Text>
    </AnchorPane>
</VBox>
